{
  "expo": {
    "name": "Qualidade e Vida App",
    "slug": "qualidade-vida-mobile",
    "privacy": "public",
    "sdkVersion": "39.0.0",
    "platforms": ["ios", "android"],
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "expo.ts",
        "expo.tsx",
        "expo.js",
        "expo.jsx",
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "wasm",
        "svg"
      ]
    },
    "version": "3.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.alatar.qualidadeVidaApp",
      "buildNumber": "3.0.2",
      "supportsTablet": true
    },
    "android": {
      "package": "com.alatar.qualidadeVidaApp",
      "versionCode": 16,
      "permissions": []
    }
  }
}
